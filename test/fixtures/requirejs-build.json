{
    "default": {
        "mainConfigFile": "setup.js",
        "baseUrl": "",
        "output": "out",
        "optimize": "uglify2",
        "excludeModules": ["libs", "shared"]
    },
    "modules": {
        "libs": {
            "include": [
                "jquery",
                "lodash"
            ],
            "excludeModules": null
        },
        "shared": {
            "optimize": "uglify",
            "include": [
                "utils"
            ],
            "excludeModules": ["libs"]
        },
        "instagram": {
            "directory": "instagram",
            "filters": {
                "desktop": "!(*.mobile.*)",
                "mobile": "*.mobile.*",
                "all": "*"
            }
        },
        "excludeModulesFaulty": {
            "excludeModules": {},
            "exclude": true
        },
        "excludeNotArray": {
            "excludeModules": ["libs"],
            "exclude": true
        },
        "excludeMissingModule": {
            "excludeModules": ["missing-module"]
        },
        "directory": {
            "directory": "directory"
        },
        "invalidDirectory": {
            "directory": {}
        },
        "missingDirectory": {
            "directory": "missing-directory"
        },
        "fileAsDirectory": {
            "directory": "requirejs-build.json"
        },
        "includeNotArray": {
            "directory": "directory",
            "include": {}
        },
        "rawTextFaulty": {
            "rawText": []
        }
    }
}
