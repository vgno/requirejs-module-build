{
    "default": {
        "mainConfigFile": "public/js/setup.js",
        "baseUrl": "public/js/",
        "output": "public/js/compiled/",
        "optimize": "uglify2",
        "excludeModules": ["libs", "shared", "admin"]
    },
    "modules": {
        "libs": {
            "include": [
                "jquery",
                "lodash"
            ],
            "excludeModules": []
        },
        "shared": {
            "optimize": "uglify",
            "include": [
                "utils"
            ],
            "excludeModules": ["libs"]
        },
        "instagram": {
            "directory": "modules/instagram",
            "subModules": {
                "mobile": ".mobile."
            }
        }
    }
}
